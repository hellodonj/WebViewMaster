<resources>
    <declare-styleable name="SegmentedControlView">
        <attr name="scv_unselectedTextColor" format="reference"/>
        <attr name="scv_unselectedColor" format="reference"/>
        <attr name="scv_selectedColor" format="reference"/>
        <attr name="scv_selectedTextColor" format="reference"/>
        <attr name="scv_items" format="reference"/>
        <attr name="scv_values" format="reference"/>
        <attr name="scv_equalWidth" format="boolean"/>
        <attr name="scv_stretch" format="boolean"/>
        <attr name="scv_defaultSelection" format="integer"/>
        <attr name="scv_identifier" format="string"/>
    </declare-styleable>
    <!--饼状图-->
    <declare-styleable name="StrokeTextView">
        <!-- 自定义的属性名称 和对应的单位 -->
        <attr name="outerColor" format="color|reference" />
        <attr name="strokeWideth" format="dimension" />
    </declare-styleable>

    <!--自定义圆点,方点-->
    <declare-styleable name="DrawPointView">
        <!-- 自定义的属性名称 和对应的单位 -->
        <attr name="pointColor" format="color|reference" />
        <attr name="pointWideth" format="dimension" />

        <attr name="pointShape" format="enum" >
        <enum name="square" value="0" />
        <enum name="round" value="1" />
        </attr>
    </declare-styleable>
</resources>