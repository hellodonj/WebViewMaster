<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle"
            android:useLevel="true">

            <stroke
                android:width="3dp"
                android:color="#ffaaaaaa" />

            <corners
                android:radius="2dp" />
        </shape>
    </item>

    <item
        android:top="1dp"
        android:left="1dp"
        android:bottom="1dp"
        android:right="1dp">
        <shape
            android:shape="rectangle"
            android:useLevel="true">

            <stroke
                android:width="2dp"
                android:color="#ffeeeeee" />

            <corners
                android:radius="2dp" />
        </shape>
    </item>
</layer-list>

